{
  "version": "1.6.5",
  "name": "@dreamer/foundry",
  "description": "Foundry deployment and verification utilities for Deno and Bun",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts",
    "./cli": "./src/cli.ts",
    "./setup": "./src/setup.ts",
    "./deploy": "./src/deploy.ts",
    "./verify": "./src/verify.ts",
    "./utils": "./src/utils/mod.ts",
    "./utils/env": "./src/utils/env.ts",
    "./utils/web3": "./src/utils/web3.ts",
    "./utils/logger": "./src/utils/logger.ts",
    "./utils/time": "./src/utils/time.ts"
  },
  "publish": {
    "exclude": [
      "tests/",
      "example/",
      ".cursor/",
      "*.test.ts",
      "*.test.js"
    ]
  },
  "imports": {
    "@dreamer/web3": "jsr:@dreamer/web3@^1.0.6-beta.8",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.0-beta.9",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.0-beta.22",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.40",
    "@dreamer/console": "jsr:@dreamer/console@^1.0.3-beta.6"
  },
  "test": {
    "exclude": [
      "tests/data/**",
      "example/**"
    ]
  },
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 100,
      "useTabs": false
    }
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "nodeModulesDir": "auto",
  "allowScripts": {
    "allow": [],
    "deny": [
      "npm:esbuild@0.27.2",
      "npm:puppeteer@24.36.0"
    ]
  }
}
